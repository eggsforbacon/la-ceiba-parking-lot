          <TableColumn fx:id="userIDCOL" minWidth="0.0" prefWidth="75.0" text="Identificación" />
            <TableColumn fx:id="userEnabledCOL" minWidth="0.0" prefWidth="75.0" text="Habilitado" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
